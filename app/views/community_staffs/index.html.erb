<p id="notice"><%= notice %></p>

<h1>Community Staffs</h1>

<table>
  <thead>
    <tr>
      <th>Staff</th>
      <th>Community</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @community_staffs.each do |community_staff| %>
      <tr>
        <td><%= community_staff.staff %></td>
        <td><%= community_staff.community %></td>
        <td><%= link_to 'Show', community_staff %></td>
        <td><%= link_to 'Edit', edit_community_staff_path(community_staff) %></td>
        <td><%= link_to 'Destroy', community_staff, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Community Staff', new_community_staff_path %>
