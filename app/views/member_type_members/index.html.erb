<p id="notice"><%= notice %></p>

<h1>Member Type Members</h1>

<table>
  <thead>
    <tr>
      <th>Member</th>
      <th>Member type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @member_type_members.each do |member_type_member| %>
      <tr>
        <td><%= member_type_member.member %></td>
        <td><%= member_type_member.member_type %></td>
        <td><%= link_to 'Show', member_type_member %></td>
        <td><%= link_to 'Edit', edit_member_type_member_path(member_type_member) %></td>
        <td><%= link_to 'Destroy', member_type_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member Type Member', new_member_type_member_path %>
