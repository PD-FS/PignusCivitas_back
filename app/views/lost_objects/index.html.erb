<p id="notice"><%= notice %></p>

<h1>Lost Objects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Date</th>
      <th>Notes</th>
      <th>Returned to</th>
      <th>Return date</th>
      <th>Phone</th>
      <th>Community</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lost_objects.each do |lost_object| %>
      <tr>
        <td><%= lost_object.name %></td>
        <td><%= lost_object.date %></td>
        <td><%= lost_object.notes %></td>
        <td><%= lost_object.returned_to %></td>
        <td><%= lost_object.return_date %></td>
        <td><%= lost_object.phone %></td>
        <td><%= lost_object.community %></td>
        <td><%= link_to 'Show', lost_object %></td>
        <td><%= link_to 'Edit', edit_lost_object_path(lost_object) %></td>
        <td><%= link_to 'Destroy', lost_object, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lost Object', new_lost_object_path %>
