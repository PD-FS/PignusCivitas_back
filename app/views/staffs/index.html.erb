<p id="notice"><%= notice %></p>

<h1>Staffs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Admission date</th>
      <th>Notes</th>
      <th>Address</th>
      <th>Staff</th>
      <th>Staff job</th>
      <th>Community</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @staffs.each do |staff| %>
      <tr>
        <td><%= staff.name %></td>
        <td><%= staff.phone %></td>
        <td><%= staff.admission_date %></td>
        <td><%= staff.notes %></td>
        <td><%= staff.address %></td>
        <td><%= staff.staff %></td>
        <td><%= staff.staff_job %></td>
        <td><%= staff.community %></td>
        <td><%= link_to 'Show', staff %></td>
        <td><%= link_to 'Edit', edit_staff_path(staff) %></td>
        <td><%= link_to 'Destroy', staff, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Staff', new_staff_path %>
