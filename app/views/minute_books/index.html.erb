<p id="notice"><%= notice %></p>

<h1>Minute Books</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Generated by</th>
      <th>Notes</th>
      <th>File</th>
      <th>Community</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @minute_books.each do |minute_book| %>
      <tr>
        <td><%= minute_book.date %></td>
        <td><%= minute_book.generated_by %></td>
        <td><%= minute_book.notes %></td>
        <td><%= minute_book.file %></td>
        <td><%= minute_book.community %></td>
        <td><%= link_to 'Show', minute_book %></td>
        <td><%= link_to 'Edit', edit_minute_book_path(minute_book) %></td>
        <td><%= link_to 'Destroy', minute_book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Minute Book', new_minute_book_path %>
