<p id="notice"><%= notice %></p>

<h1>Asset Stock Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @asset_stock_statuses.each do |asset_stock_status| %>
      <tr>
        <td><%= asset_stock_status.name %></td>
        <td><%= asset_stock_status.description %></td>
        <td><%= link_to 'Show', asset_stock_status %></td>
        <td><%= link_to 'Edit', edit_asset_stock_status_path(asset_stock_status) %></td>
        <td><%= link_to 'Destroy', asset_stock_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Asset Stock Status', new_asset_stock_status_path %>
