<p id="notice"><%= notice %></p>

<h1>Contracts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Date</th>
      <th>Expiration</th>
      <th>Type</th>
      <th>Notes</th>
      <th>Contractor</th>
      <th>Hired</th>
      <th>Contract status</th>
      <th>Community</th>
      <th>Security company</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contracts.each do |contract| %>
      <tr>
        <td><%= contract.title %></td>
        <td><%= contract.date %></td>
        <td><%= contract.expiration %></td>
        <td><%= contract.type %></td>
        <td><%= contract.notes %></td>
        <td><%= contract.contractor %></td>
        <td><%= contract.hired %></td>
        <td><%= contract.contract_status %></td>
        <td><%= contract.community %></td>
        <td><%= contract.security_company %></td>
        <td><%= link_to 'Show', contract %></td>
        <td><%= link_to 'Edit', edit_contract_path(contract) %></td>
        <td><%= link_to 'Destroy', contract, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contract', new_contract_path %>
